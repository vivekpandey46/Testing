
public class Pattern5 {
	
	public static void main(String[] args) {
		
		int n=5;
		
		for(int i=1;i<=n;i++)
		{
			for (int j=n;j>=i;j--)
			{
				System.out.print(j);
			}
			
			System.out.println();
		}
		
		for (int k=2;k<=n;k++)
		{
			for (int l=1;l<=k;l++)
			{
				System.out.print(l);
			}
			
			System.out.println();
		}
	}

}
